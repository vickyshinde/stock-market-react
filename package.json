{
  "name": "stock-market-react",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.13.0"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.11.14",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "i18next": "22.4.14",
    "i18next-browser-languagedetector": "7.0.1",
    "i18next-http-backend": "2.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.2.0",
    "react-router-dom": "6.9.0",
    "react-scripts": "5.0.1",
    "web-vitals": "2.1.4",
    "husky": "8.0.3"
  },
  "scripts": {
    "start": "PORT=3030 react-scripts start",
    "start-win": "SET PORT=3030 && react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --write ./**/*.{js,jsx,ts,tsx,css} --config ./.prettierrc",
    "lint:ts:fix": "eslint src/**/*.js src/**/*.jsx --fix",
    "lint:ts": "eslint src/**/*.js src/**/*.jsx",
    "test": "react-scripts test",
    "test:options": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-staged": "lint-staged",
    "precommit": "npm run lint:staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.8.7"
  }
}
